<h1>Post New!</h1>
<%= link_to "Back to Post", posts_path %>
<%= form_for @post do |f| %>
  <p><%= f.label :title %>
  <%= f.text_field :title %></p><br>
  <p><%= f.label :destination_id %>
  <%= f.collection_select :destination_id, Destination.all, :id, :name  %></p><br>
  <p><%= f.label :blogger_id %>
  <%= f.collection_select :blogger_id, Blogger.all, :id, :name %></p><br>
  <p><%= f.label :content %>
  <%= f.text_area :content %></p><br>
  <%= f.submit %>

<% end %>
<br>
